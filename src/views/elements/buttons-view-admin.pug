span.service-buttons-wrapper.service-buttons-wrapper-admin
    for order in softwareListDetails
        a.btn.btn-primary.btn-sm.btn-orderedit(href="edit?tsid="+ order.applicationid class="link-id")
                | Ethikantrag bearbeiten
                i.fas.fa-edit
        button.pdfexport.btn.btn-primary.btn-sm
            | Ethikantrag als PDF speichern
            i.fas.fa-file-pdf
        button.btn.btn-primary.btn-sm.hidden-xs(type='button' data-bs-toggle='modal' data-bs-target='#removeModal')
            | Ethikantrag l&ouml;schen
            i.fas.fa-trash-alt
        #removeModal.modal(tabindex='-1' role='dialog' aria-labelledby='ModalCenterTitle' aria-hidden='true')
            .modal-dialog.modal-dialog-centered(role='document')
                .modal-content
                    .modal-header
                        h5.modal-title Ethikantrag l&ouml;schen
                    .modal-body
                        | Wollen Sie wirklich den Ethikantrag unwiderruflich l&ouml;schen?
                    .modal-footer
                        button.btn.btn-secondary.btn-sm(type='button' data-bs-dismiss="modal") Abbrechen
                        for order in softwareListDetails
                            a.btn.btn-primary.btn-sm.btn-remove.link-id-delete(href="#")
                                | Ethikantrag l&ouml;schen
        for order in softwareListDetails
            a(href="history?tsid="+ order.applicationid class="link-id").btn.btn-primary.btn-sm
                    | Detaillierter Verlauf des Antrags
                    i.fas.fa-history
        a(href='user').btn.btn-primary.btn-sm.floatright
            | Zur&uuml;ck zu
            | allen Antr&auml;gen
            i.fas.fa-search
