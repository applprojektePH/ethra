script(type='text/javascript').
    $(document).ready(function () {
        if (window.location.href.indexOf("form") > -1) {
            $('#buttonsubmitformdraft').click(function (event) {
                event.stopPropagation();
                $('#formOrder').removeClass('needs-validation');
                $('#form-status-input').attr('value', '10');
            });

        } else if (window.location.href.indexOf("edit") > -1) {
                $("form").find(".form-select").each(function () {
                        let attrselectedform = $(this).attr('filled');
                $(this).find("option[value='" + attrselectedform + "']").prop('selected', true);
                });
                $(".form-check-wrapper").each(function () {
                        let attrselectedform = $(this).attr('filled');
                        let nameArr = attrselectedform.split(',');
                        for (let i = 0; i < nameArr.length; i++) {
                                $('input:checkbox[value="' + nameArr[i] + '"]').attr('checked', true);
                }
                });
            $('#buttonsubmitformdraft').click(function (event) {
                event.stopPropagation();
                $('#form-status-input').attr('value', '1');
            });
            $('#buttonsubmitsavechanges').click(function () {
                $('#form-status-input').attr('value', '0');
            });
            //only edit and form functions
                    var maxChar = parseInt($('.textarea-js').attr('maxlength'));
                    $('.textarea-js[maxlength]').parent().find('.charleft').html(maxChar - $(this).val().length);
                    var char = $('.textarea-js').val().length;
                    $('.textarea-js').parent().find('.charleft').html(maxChar - char);
                    $('.textarea-js[maxlength]').keyup(function(){
                        if($(this).val().length > maxChar){
                            $(this).val($(this).val().substr(0, maxChar));
                        }
                        $(this).parent().find('.charleft').html(maxChar - $(this).val().length);
                    });
        }
        $('.link-edit-back').click(function () {
            const urlParams = new URLSearchParams(window.location.search);
            const currenturlParam = urlParams.get('tsid');
            window.location.replace("/ethra/details?tsid=" + currenturlParam);
        })
        $(".custom-control-select").on('change', function (e) {
            var valueSelected = $('option:selected', this).attr('answer');
            if (valueSelected == "open") {
                $(this).nextAll(".inputHide").delay(300).slideDown("slow");
            } else {
                $(this).nextAll(".inputHide").delay(300).slideUp("slow");
            }
        });
        // $(".custom-control-label").click(function () {
        //     if ($(this).attr('answer') == "open") {
        //         $(this).closest(".custom-control").nextAll(".inputHide").delay(300).slideDown("slow");
        //     } else {
        //         $(this).closest(".custom-control").nextAll(".inputHide").delay(300).slideUp("slow");
        //     }
        // });
        $(".custom-control-label-js").click(function () {
            if ($(this).attr('answer') == "open") {
                $(this).closest(".custom-control").next(".inputHide").delay(300).slideDown("slow");
            } else {
                $(this).closest(".custom-control").prev(".inputHide").delay(300).slideUp("slow");
            }
        });
        // $(".custom-control-label-js2").click(function () {
        //     if ($(this).attr('answer') == "open") {
        //         $(this).closest(".custom-control").nextAll(".inputHide2").delay(300).slideDown("slow");
        //     } else {
        //         $(this).closest(".custom-control").nextAll(".inputHide2").delay(300).slideUp("slow");
        //     }
        //});
        let d = new Date();
        let month = d.getMonth() + 1;
        let day = d.getDate();
        let hour = d.getHours();
        let minute = d.getMinutes();
        let datetime =
            (('' + day).length < 2 ? '0' : '') + day + '.' +
            (('' + month).length < 2 ? '0' : '') + month + '.' +
            d.getFullYear() + ' ' +
            (('' + hour).length < 2 ? '0' : '') + hour + ':' +
            (('' + minute).length < 2 ? '0' : '') + minute;
        $('.form-date').text(datetime);
        $('.form-date-input').attr('value', datetime);
        let formsubmit = false;
        if ($("[name='browser']:checked").length !== 0) {
            $("[name='browser']").attr('required', false);
        }
        $(function () {
            let requiredCheckboxes = $('input:checkbox[required]');
            requiredCheckboxes.change(function () {
                if (requiredCheckboxes.is(':checked')) {
                    requiredCheckboxes.removeAttr('required');
                } else {
                    requiredCheckboxes.attr('required', 'required');
                }
            });
        });
        $("#accordionForm .accordion-item").find("input, select, textarea").focus(function () {
            let current = $(this).closest(".accordion-collapse").prev();
            current.removeClass('required-header');
        });
        $(".submitformbtnaction").find("input, select, textarea").focus(function () {
            let current = $(this).closest(".accordion-collapse").prev();
            current.removeClass('required-header');
        });
        $(".submitformbtnaction").on("click", function (event) {
            event.stopPropagation();
            $("#headingThree").removeClass('required-header');
            $("#accordionForm").find(".accordion-item").each(function () {
                $(this).find("input, textarea, select").filter('[required]').each(function () {
                    if (($(this).val() == " ") || ($(this).val() == null) || ($.trim($(this).val()).length == 0)) {
                        let current = $(this).closest(".accordion-collapse").prev().eq(0);
                        current.addClass('required-header');
                        return false;
                    }
                    if (($("input[name='softwareupdate']:checked").length !== 0)) {
                        $('#headingThree').removeClass('required-header');
                    }
                    if (($("input[name='browser']:checked").length == 0)) {
                        $("input[name='browser']").closest(".accordion-collapse").prev().addClass('required-header');
                    } else if (($("input[name='softwareupdate']:checked").length == 0)) {
                        $("input[name='softwareupdate']").closest(".accordion-collapse").prev().addClass('required-header');
                        $('#headingThree').removeClass('required-header');
                    } else if (($("input[name='softwareinstitut']:checked").length == 0)) {
                        $("input[name='softwareinstitut']").closest(".accordion-collapse").prev().addClass('required-header');
                        $('#headingThree').removeClass('required-header');
                    } else if (($("input[name='softwarehochschinteresse']:checked").length == 0)) {
                        $("input[name='softwarehochschinteresse']").closest(".accordion-collapse").prev().addClass('required-header');
                        $('#headingThree').removeClass('required-header');
                    } else if (($("input[name='cloud']:checked").length == 0)) {
                        $("input[name='cloud']").closest(".accordion-collapse").prev().addClass('required-header');
                        $('#headingThree').removeClass('required-header');
                    } else {
                        let current = $(this).closest(".accordion-collapse").prev().eq(0);
                        current.removeClass('required-header');
                        return true;
                    }
                });
            });
            formsubmit = true;
            if (formsubmit == true) {
                (function () {
                    'use strict'
                    // Fetch all the forms we want to apply custom Bootstrap validation styles to
                    var forms = document.querySelectorAll('.needs-validation')
                    // Loop over them and prevent submission
                    Array.prototype.slice.call(forms)
                        .forEach(function (form) {
                            form.addEventListener('submit', function (event) {
                                if (!form.checkValidity()) {
                                    event.preventDefault()
                                    event.stopPropagation()
                                }
                                form.classList.add('was-validated')
                            }, false)
                        })
                })()
                formsubmit = false;
            }
        })

        $('.accordion-collapse').on('shown.bs.collapse', function (e) {
            var $panel = $(this).closest('.accordion-item');
            let offset;
            if ($(window).width() > 992) {
                offset = 120
            }
            else if (($(window).width() < 992) && ($(window).width() > 768)) {
                offset = 150
            } else if ($(window).width() < 768)  {
                offset = 300
            }
                $('html,body').animate({
                    scrollTop: $panel.offset().top - offset
                }, 200);

        });

    });